{
  "integrations": {
  },
  "update_behavior": {
    "auto_start_updates": "no",
    "skip_manual_testing": "no",
    "source_branch": "local-dev",
    "destination_branch": "updates/[task:id]"
  },
  "events": {
    "test_passed": [
      {
        "merge_branch": {
          "source": "updates/[task:id]",
          "delete_after_merge": "yes",
          "target": [
            "local-dev"
          ]
        }
      }
    ]
  },
  "extra": {
      "update_policy": "patch",
      "dockerfiles" : [
        "Dockerfile.prod",
        "Dockerfile.stage"
      ],
      "policies": {
        "images": {
          "mongo": "patch",
          "nginx": "none"
        },
        "files": {
          "app1/Dockerfile": {
           "nginx11": "none"
          }
        }
      }
  }
}
